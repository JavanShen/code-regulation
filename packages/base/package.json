{
    "name": "@beaglee/base",
    "version": "1.0.0-alpha.0",
    "type": "module",
    "description": "tool to build eslint+prettier",
    "scripts": {
        "dev": "tsc-watch --onSuccess \"node scripts/out.js\"",
        "build": "tsc && node scripts/out.js"
    },
    "bin": {
        "rec": "./dist/bin/rec.js"
    },
    "files": [
        "dist"
    ],
    "keywords": ["eslint", "prettier", "regulation", "tool"],
    "author": "apine",
    "license": "MIT",
    "dependencies": {
        "@beaglee/eslint-config-base": "workspace:^1.0.0-alpha.0",
        "@beaglee/eslint-config-typescript": "workspace:^1.0.0-alpha.0",
        "@beaglee/eslint-config-vue": "workspace:^1.0.0-alpha.0",
        "@beaglee/prettier": "workspace:^1.0.0-alpha.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
